<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color"
    android:orientation="vertical">

    <include layout="@layout/top_bar_layout" />

    <TextView
        android:id="@+id/tv_time"
        style="@style/textViewStyle.White.PaddingLeftRight_5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height_44"
        android:layout_marginTop="@dimen/margin_10"
        android:background="@drawable/shape_null_white_5"
        android:gravity="center"
        android:text="时间选择器展示" />

    <TextView
        android:id="@+id/tv_cus_text"
        style="@style/textViewStyle.White.PaddingLeftRight_5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height_44"
        android:layout_marginTop="@dimen/margin_10"
        android:background="@drawable/shape_null_white_5"
        android:gravity="center"
        android:text="自定义文本收起/全文" />

    <TextView
        android:id="@+id/tv_tree"
        style="@style/textViewStyle.White.PaddingLeftRight_5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height_44"
        android:layout_marginTop="@dimen/margin_10"
        android:background="@drawable/shape_null_white_5"
        android:gravity="center"
        android:text="三级列表" />

    <TextView
        android:id="@+id/tv_dialog"
        style="@style/textViewStyle.White.PaddingLeftRight_5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height_44"
        android:layout_marginTop="@dimen/margin_10"
        android:background="@drawable/shape_null_white_5"
        android:gravity="center"
        android:text="dialog窗口" />

    <TextView
        android:id="@+id/tv_img"
        style="@style/textViewStyle.White.PaddingLeftRight_5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height_44"
        android:layout_marginTop="@dimen/margin_10"
        android:background="@drawable/shape_null_white_5"
        android:gravity="center"
        android:text="照片选择器" />

    <TextView
        android:id="@+id/tv_map"
        style="@style/textViewStyle.White.PaddingLeftRight_5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height_44"
        android:layout_marginTop="@dimen/margin_10"
        android:background="@drawable/shape_null_white_5"
        android:gravity="center"
        android:text="跳转第三方地图(高德、百度、腾讯地图)" />

    <TextView
        android:id="@+id/tv_rsa"
        style="@style/textViewStyle.White.PaddingLeftRight_5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item_height_44"
        android:layout_marginTop="@dimen/margin_10"
        android:background="@drawable/shape_null_white_5"
        android:gravity="center"
        android:text="RSA工具" />


</LinearLayout>